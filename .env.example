# Copy to `.env` and fill values. All env keys below use prefix POKEPRICER_.

POKEPRICER_DEBUG=false
POKEPRICER_LOG_LEVEL=INFO
POKEPRICER_DATA_DIR=./data

# API credentials (leave empty until available)
POKEPRICER_TCGPLAYER_PUBLIC_KEY=
POKEPRICER_TCGPLAYER_PRIVATE_KEY=
POKEPRICER_EBAY_APP_ID=
POKEPRICER_PSA_API_KEY=
POKEPRICER_CGC_API_KEY=

# Optional DB (e.g. Postgres). Example:
# postgresql://user:password@localhost:5432/poke_pricer
POKEPRICER_POSTGRES_DSN=
